{
  "manifest_version": 3,
  "name": "SPINE",
  "version": "1.0",
  "description": "Supporting Propagandists Is Not Ethical",
  "permissions": ["scripting"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"], 
      "js": ["content.js"], 
      "run_at": "document_idle"
    }
  ]
}
